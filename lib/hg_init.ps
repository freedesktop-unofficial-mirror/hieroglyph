% Initialize against current language level.
systemdict /languagelevel known {
    languagelevel dup 1 eq {
	(hg_init_l1.ps)
    } {
	2 eq {
	    (hg_init_l2.ps)
	} {
	    (hg_init_l3.ps)
	} ifelse
    } ifelse
} {
    (hg_init_l1.ps)
} ifelse
(r) file cvx exec
